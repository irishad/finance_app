<p style="color: green"><%= notice %></p>

<h1>Wallets</h1>

<div id="wallets">
  <% @wallets.each do |wallet| %>
    <%= render wallet %>
    <p>
      <%= link_to "Show this wallet", wallet, class: "btn btn-success" %>
    </p>
  <% end %>
</div>

<%= link_to "New wallet", new_wallet_path, class: "btn btn-success" %>
